<!DOCTYPE html>
<html lang="sv">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <link rel="stylesheet" href="~/css/style.css">
    <title>@ViewData["Title"]</title>
</head>
<body>
    <div class="flexWraper">
    <header>
        <!--Partial används för att infoga menyn-->
        @Html.Partial("_MenuBar")
    </header>
    <div class="container">
        <main>
            @RenderBody()
        </main>
    </div>
    <footer>
        <!-- sessionsvariabel -->
        <p class="date"> Dagens datum: @ViewData["today"]</p>
    </footer>
    </div>
</body>
</html>